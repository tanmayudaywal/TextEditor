<AnchorPane id="AnchorPane" fx:id="settingAnchorPaneFXID" maxWidth="600.0" styleClass="jfx-settingAnchorPaneFXID" stylesheets="@settingstyles.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="text.editor.FXMLSettingController">
