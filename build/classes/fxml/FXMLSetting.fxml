                     <children>
                        <Label fx:id="settingHelpLabelFXID" onMouseEntered="#settingHelpAboutCheckForUpdateLabelOnMouseEntered" text="Help">
                           <HBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </HBox.margin>
                        </Label>
                        <Label fx:id="settingAboutLabelFXID" onMouseEntered="#settingHelpAboutCheckForUpdateLabelOnMouseEntered" text="About">
